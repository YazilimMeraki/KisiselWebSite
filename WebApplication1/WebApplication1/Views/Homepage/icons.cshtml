@using WebApplication1.Models.Classes
@model List<Icon>

<div>

    @foreach (var x in Model)
    {
        <a href="@x.Link" ><i class="@x.IconName"></i></a>

    }
    <style>
        a{
            font-size:25px;
            padding-left:15px;
            transition:1s;


        }
        a:hover{
            font-size:30px;
              

        }
    </style>
</div>


